class Solution {
    public int singleNumber(int[] nums) {

        int result = 0;
        int count = 0;

        for(int i = 0; i < nums.length; i++){
            for(int j = i+1; j < nums.length; j++)
            if(nums[i] == nums[j]){
                nums[i] = 0;
                nums[j] = 0;

            }
        }
        
        for(int k=0; k < nums.length; k++){
            if(nums[k] != 0){
                result = nums[k];
            }
        }

        return result;
    }
}
